{
  "name": "parcel-sass-starter",
  "version": "1.0.0",
  "description": "A parcel bundler starter template for scss, scss maps with font pairings and color maps",
  "main": "src/**/*.html",
  "scripts": {
    "dev": "parcel src/**/*.html --no-cache --port 8080 --dist-dir tmp",
    "deploy": "parcel build src/**/*.html --no-cache --no-content-hash --dist-dir dist"
  },
  "author": "Altus Janse Van Rensburg <albertusbjvr@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "parcel": "^2.0.0-beta.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AltusJVR/parcel-starter.git"
  },
  "keywords": [
    "parcel starter",
    "parcel sass starter",
    "sass maps",
    "sass maps with Google fonts",
    "sass partials layout",
    "Dart sass"
  ],
  "bugs": {
    "url": "https://github.com/AltusJVR/parcel-starter/issues"
  },
  "homepage": "https://github.com/AltusJVR/parcel-starter#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-beta.2"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ]
}
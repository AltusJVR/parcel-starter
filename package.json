  "license": "MIT",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "sass": "^1.32.8",
    "parcel": "1.12.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AltusJVR/parcel-starter.git"
  },
  "keywords": [
    "parcel starter",
    "parcel sass starter",
    "sass maps",
    "sass maps with Google fonts",
    "sass partials layout",
    "Dart sass"
  ],
  "bugs": {
    "url": "https://github.com/AltusJVR/parcel-starter/issues"
  },
  "homepage": "https://github.com/AltusJVR/parcel-starter#readme"

{
  "name": "parcel-sass-starter",
  "version": "1.0.0",
  "description": "- `npm run dev` - starts a dev server watches for file changes.\r -  `npm run build` - build and minifies files into the dist folder.",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/**/*.html --no-cache --port 8080 --out-dir tmp",
    "deploy": "parcel build src/**/*.html --no-cache --out-dir dist"
  },
